@import '../../../sky.wxss';

.text {
  font-size: 14px;
}

.timeTop {
  display: flex;
  flex-direction: row;
  padding: 30rpx 0;
}

.timeTop text {
  width: 50%;
  display: inline-block;
}

.timeleft {
  padding-left: 30rpx;
}

.timeright {
  text-align: right;
  padding-right: 30rpx;
}

.tiptext {
  font-size: 12px;
  color: #999;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx 3%;
}
