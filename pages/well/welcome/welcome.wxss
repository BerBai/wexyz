@import '../../../sky.wxss';

.tips{
  font-size: 14px;
  color: hsl(0, 0%, 40%);
  margin: 20rpx 40rpx;
}

.footer{
    display: flex;
    flex-direction: column;
    font-size: 9pt;
    line-height: 150%;
    text-align: center;
    padding: 50rpx 0 15rpx;
    color: #c2c2c2;
}
.footer-well{
    font-size: 10pt;
    height: 10pt;
    line-height: 10pt;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 8rpx;
}
.footer-well image{
    width: 10pt;
    height: 10pt;
    margin-right: 5rpx;
}
